func int[] bubbleSort(int[] a, int n){
  int i;
  int j;
  int tmp;

  for (i = 0; i < n-1; i = i+1){
    for (j = 0; j < n-i-1; j = j+1){
      if (a[j] > a[j+1]){
        tmp = a[j];
        a[j] = a[j+1];
        a[j+1] = tmp;
        printa(a);
      }
    } 
  } 
  printa(a);
  return a;
}

func int[] selectionSort(int[] a, int n){
  int i;
  int j;
  int tmp;
  int min_idx;

  for (i = 0; i < n-1; i = i+1){
    min_idx = i;

    for (j = i+1; j < n; j = j+1){
      if (a[j] < a[min_idx]){
        min_idx = j;
      } 
    }  

    tmp = a[min_idx];
    a[min_idx] = a[i];
    a[i] = tmp;   
    printa(a);
  }   
  printa(a);
  return a;
}

